{
  "name": "@addresszen/openapi",
  "description": "AddressZen OpenAPI v3 specifications",
  "version": "3.4.0",
  "devDependencies": {
    "@cablanchard/semantic-release": "~2.0.0",
    "@cablanchard/tsconfig": "~2.0.0",
    "@redocly/cli": "~2.0.7",
    "prettier": "~3.6.2",
    "semantic-release": "~24.2.7",
    "typescript": "~4.7.3"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release --no-ci",
    "start": "redocly preview --project-dir dist",
    "dev": "redocly preview --project-dir dist",
    "replace": "ts-node replace.ts",
    "build": "tsc",
    "test": "redocly lint dist/openapi.yaml && redocly lint dist/openapi.json"
  },
  "main": "dist/openapi.js",
  "types": "dist/openapi.d.ts",
  "author": {
    "name": "Addressen",
    "email": "support@addresszen.com",
    "url": "https://addresszen.com"
  },
  "bugs": {
    "url": "https://github.com/addresszen/openapi/issues",
    "email": "support@addresszen.com"
  },
  "homepage": "https://github.com/addresszen/openapi",
  "repository": {
    "type": "git",
    "url": "https://github.com/addresszen/openapi.git"
  },
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm",
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      "next"
    ]
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "AddressZen",
    "OpenAPI",
    "Address Verification",
    "Address Lookup"
  ],
  "dependencies": {
    "@ideal-postcodes/openapi": "4.12.0",
    "replace-in-file": "~6.3.2",
    "ts-node": "~10.7.0"
  }
}
